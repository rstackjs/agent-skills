{
  "name": "rstest-cdp",
  "version": "0.1.0",
  "private": true,
  "description": "CDP-based debugger CLI for rstest, used by agent-skills.",
  "repository": {
    "type": "git",
    "url": "https://github.com/rstackjs/agent-skills",
    "directory": "packages/rstest-cdp"
  },
  "type": "module",
  "bin": "./dist/rstest-cdp.cjs",
  "files": [
    "dist",
    "schema"
  ],
  "scripts": {
    "build": "rslib build",
    "postbuild": "pnpm run gen:schema",
    "dev": "rslib build --watch",
    "gen:schema": "node --experimental-strip-types ./scripts/genPlanSchema.mts && pnpm prettier --write './schema/plan.schema.json'",
    "prepare": "pnpm run gen:schema",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@jridgewell/trace-mapping": "0.3.31",
    "@rslib/core": "^0.19.0",
    "@types/ws": "^8.18.1",
    "@valibot/to-json-schema": "^1.2.0",
    "cac": "^6.7.14",
    "json-rpc-2.0": "^1.7.1",
    "typescript": "^5.9.3",
    "valibot": "^1.2.0",
    "ws": "^8.18.3"
  },
  "engines": {
    "node": ">=18.12.0"
  }
}
